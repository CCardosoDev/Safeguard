<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InsertUser</servlet-name>
        <servlet-class>api.InsertUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertProfile</servlet-name>
        <servlet-class>api.InsertProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveProfiles</servlet-name>
        <servlet-class>api.RemoveProfiles</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveUser</servlet-name>
        <servlet-class>api.RemoveUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfile</servlet-name>
        <servlet-class>api.EditProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetProfiles</servlet-name>
        <servlet-class>api.GetProfiles</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertAndroidID</servlet-name>
        <servlet-class>api.InsertAndroidID</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertUser</servlet-name>
        <url-pattern>/InsertUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertProfile</servlet-name>
        <url-pattern>/InsertProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveProfiles</servlet-name>
        <url-pattern>/RemoveProfiles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveUser</servlet-name>
        <url-pattern>/RemoveUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfile</servlet-name>
        <url-pattern>/EditProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetProfiles</servlet-name>
        <url-pattern>/GetProfiles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertAndroidID</servlet-name>
        <url-pattern>/InsertAndroidID</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <security-constraint>
        <display-name>ConstraintHTTPS</display-name>
        <web-resource-collection>
            <web-resource-name>ProfilesServiceResource</web-resource-name>
            <description/>
            <url-pattern>/*</url-pattern>
        </web-resource-collection>
        <user-data-constraint>
            <description/>
            <transport-guarantee>CONFIDENTIAL</transport-guarantee>
        </user-data-constraint>
    </security-constraint>
</web-app>
